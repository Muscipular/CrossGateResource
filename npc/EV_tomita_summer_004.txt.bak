#　　　뼈물고기 코츠하치의 권

###　청량의 팔찌를 소지

block Item_607927 >= 1
	EndWindow 8400001



###　청량의 팔찌를 소지하지 않음

block
	Window 8400002,YES,NO

		Window_YES Gold < 88
			EndWindow 8400003

		Window_YES ItemSpace < 1
			EndWindow 8400004

		Window_YES Item_607927 >= 1
			EndWindow 8400005

		Window_YES
			Window 8400006,OK

				Window_OK Gold < 88
					EndWindow 8400007

				Window_OK ItemSpace < 1
					EndWindow 8400008

				Window_OK Item_607927 >= 1
					EndWindow 8400009

				Window_OK
					AddGold -88
					GiveItemEX 607927,1,"\n\n　소지품이 가득 찼다코츠."

		Window_NO
			EndWindow 8400010





